            <method name="open"><![CDATA[
importPackage(Packages.com.hhsc.rpt);

ejbClient = new ItemFrameReport();
ejbClient.setEJB(params["JNDIName"]);

master = ejbClient.getEntity(params["id"]);

currentRow = 0;]]></method>
            <method name="fetch"><![CDATA[
if(currentRow == 0){

	dataSetRow["formid"] = master.getFormid();
	dataSetRow["formdate"] = master.getFormdate();	
	dataSetRow["formtype"] = master.getFormtype();
	if(dataSetRow["formtype"]=="XZ"){
		dataSetRow["formtype"]="新增";
	}
	if(dataSetRow["formtype"]=="CG"){
		dataSetRow["formtype"]="重感";
	}
	if(dataSetRow["formtype"]=="BG"){
		dataSetRow["formtype"]="补感";
	}
	dataSetRow["designno"] = master.getItemno();
	dataSetRow["colorno"] = master.getColorno();
	dataSetRow["designspec"] = master.getDesignspec();
	dataSetRow["framespec"] = master.getFramespec();
	dataSetRow["designsets"] = master.getDesignsets();
	dataSetRow["losssets"] = master.getLosssets();
	dataSetRow["reason"] = master.getReason();
	if(dataSetRow["reason"]!=null &amp;&amp; dataSetRow["reason"]=="ZB"){
		dataSetRow["reason"]="制版";
	}
	if(dataSetRow["reason"]!=null &amp;&amp; dataSetRow["reason"]=="HG"){
		dataSetRow["reason"]="画稿";
	}
	if(dataSetRow["reason"]!=null &amp;&amp; dataSetRow["reason"]=="YH"){
		dataSetRow["reason"]="印花";
	}
	if(dataSetRow["reason"]!=null &amp;&amp; dataSetRow["reason"]=="QT"){
		dataSetRow["reason"]="其他";
	}
	dataSetRow["remark"] = master.getRemark();	
	dataSetRow["srcformid"] = master.getSrcformid();
	dataSetRow["srcseq"] = master.getSrcseq();			
	currentRow++;
	return true;
}	
						
return false;]]></method>
            <method name="close"><![CDATA[ejbClient = null;]]></method>
