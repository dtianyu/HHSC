            <property name="dataSource">scriptDS</property>
            <method name="open"><![CDATA[
importPackage(Packages.com.hhsc.rpt);

ejbClient = new SalesShipmentReport();
ejbClient.setEJB(params["JNDIName"]);

master = ejbClient.getEntity(params["id"]);
detail = ejbClient.getDetail(params["formid"]);

currentRow = 0;]]></method>
            <method name="fetch"><![CDATA[

if (currentRow < detail.size()){
	dataSetRow["formid"] = master.getFormid();
	dataSetRow["formdate"] = master.getFormdate();
	dataSetRow["formtype"] = master.getShiptype().getName();
	dataSetRow["customerno"] = master.getCustomer().getCustomerno();
	dataSetRow["customer"] = master.getCustomer().getFullname();
	dataSetRow["dept"] = master.getDept().getDept();
	dataSetRow["salesman"] = master.getSalesman().getUsername();
	dataSetRow["currency"] = master.getCurrency();
	dataSetRow["exchange"] = master.getExchange();
	dataSetRow["taxtype"] = master.getTaxtype();
	dataSetRow["taxkind"] = master.getTaxkind();
	dataSetRow["taxrate"] = master.getTaxrate();
	dataSetRow["seq"] = detail.get(currentRow).getSeq();
	dataSetRow["designno"] = detail.get(currentRow).getItemno();
	dataSetRow["designdesc"] = detail.get(currentRow).getItemmaster().getItemdesc();
						
	dataSetRow["customeritemno"] = detail.get(currentRow).getCustomeritemno();
	dataSetRow["colorno"] = detail.get(currentRow).getColorno();
	dataSetRow["customercolorno"] = detail.get(currentRow).getCustomercolorno();
	if(detail.get(currentRow).getItemBatch()!=null){
		dataSetRow["itemno"] = detail.get(currentRow).getBatch();
		dataSetRow["itemdesc"] = detail.get(currentRow).getItemBatch().getItemdesc();	
		dataSetRow["itemyarncount"] = detail.get(currentRow).getItemBatch().getItemyarncount();	
		dataSetRow["itemmake"] = detail.get(currentRow).getItemBatch().getItemmake();	
		dataSetRow["itemwidth"] = detail.get(currentRow).getItemBatch().getItemwidth();
	}	
	dataSetRow["qty"] = detail.get(currentRow).getQty();
	dataSetRow["unit"] = detail.get(currentRow).getUnit();
	dataSetRow["price"] = detail.get(currentRow).getPrice();
	dataSetRow["amts"] = detail.get(currentRow).getAmts();
	dataSetRow["srcformid"] = detail.get(currentRow).getSrcformid();
	dataSetRow["srcseq"] = detail.get(currentRow).getSrcseq();	
		
	dataSetRow["shipadd"] = master.getShipadd();
	dataSetRow["remark"] = master.getRemark();
	
	currentRow++;
	return true;		
}
return false;]]></method>
            <method name="close"><![CDATA[ejbClient = null;]]></method>
