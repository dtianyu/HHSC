            <method name="open"><![CDATA[
importPackage(Packages.com.hhsc.rpt);

ejbClient = new PurchaseRequestReport();
ejbClient.setEJB(params["JNDIName"]);

master = ejbClient.getEntity(params["id"]);
list = ejbClient.getDetail(params["formid"]);

currentRow = 0;]]></method>
            <method name="fetch"><![CDATA[
if(currentRow < 16 || currentRow < list.size()){
	if(currentRow < list.size()){
		dataSetRow["formid"] = master.getFormid();
		dataSetRow["formdate"] = master.getFormdate();
		dataSetRow["purtype"] = master.getPurtype();
		dataSetRow["dept"] = master.getDept().getDept();
		dataSetRow["user"] = master.getSystemuser().getUsername();
		dataSetRow["remark"] = master.getRemark();
		dataSetRow["seq"] = list.get(currentRow).getSeq();
		dataSetRow["itemdesign"] = "";
		dataSetRow["itemdesigndesc"] = "";
		dataSetRow["itemdesignspec"] = "";
		dataSetRow["itemdesignimg"] = "";
		dataSetRow["itemno"] = list.get(currentRow).getItemno();
		dataSetRow["colorno"] = list.get(currentRow).getColorno();
		dataSetRow["itemdesc"] = list.get(currentRow).getItemmaster().getItemdesc();
		dataSetRow["itemspec"] = list.get(currentRow).getItemmaster().getItemspec();			
		dataSetRow["itemyarncount"] = list.get(currentRow).getItemmaster().getItemyarncount();	
		dataSetRow["itemmake"] = list.get(currentRow).getItemmaster().getItemmake();	
		dataSetRow["itemwidth"] = list.get(currentRow).getItemmaster().getItemwidth();		
		dataSetRow["itemweight"] = list.get(currentRow).getItemmaster().getItemweight();	
		dataSetRow["qty"] = list.get(currentRow).getQty();		
		dataSetRow["unit"] = list.get(currentRow).getUnit();	
		dataSetRow["requestdate"] = list.get(currentRow).getRequestdate();
		dataSetRow["detailremark"] = list.get(currentRow).getRemark();			
		currentRow++;
		return true;	
	}
	dataSetRow["formid"] = master.getFormid();
	dataSetRow["formdate"] = master.getFormdate();

	currentRow++;
	return true;
}
return false;]]></method>
