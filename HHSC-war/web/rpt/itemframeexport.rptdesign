            <method name="open"><![CDATA[
importPackage(Packages.com.hhsc.rpt);

ejbClient = new ItemFrameReport();
ejbClient.setEJB(params["JNDIName"]);

detail = ejbClient.findByFilters(params["filterFields"],params["sortFields"],true);

currentRow = 0;]]></method>
            <method name="fetch"><![CDATA[
if(currentRow < detail.size()){

	dataSetRow["formid"] = detail.get(currentRow).getFormid();
	dataSetRow["formdate"] = detail.get(currentRow).getFormdate();	
	dataSetRow["formtype"] = detail.get(currentRow).getFormtype();
	if(dataSetRow["formtype"]=="XZ"){
		dataSetRow["formtype"]="新增";
	}
	if(dataSetRow["formtype"]=="CG"){
		dataSetRow["formtype"]="重感";
	}
	if(dataSetRow["formtype"]=="BG"){
		dataSetRow["formtype"]="补感";
	}
	dataSetRow["designno"] = detail.get(currentRow).getItemno();
	dataSetRow["colorno"] = detail.get(currentRow).getColorno();
	dataSetRow["designspec"] = detail.get(currentRow).getDesignspec();
	dataSetRow["framespec"] = detail.get(currentRow).getFramespec();
	dataSetRow["designsets"] = detail.get(currentRow).getDesignsets();
	dataSetRow["losssets"] = detail.get(currentRow).getLosssets();
	dataSetRow["reason"] = detail.get(currentRow).getReason();
	if(dataSetRow["reason"]!=null &amp;&amp; dataSetRow["reason"]=="ZB"){
		dataSetRow["reason"]="制版";
	}
	if(dataSetRow["reason"]!=null &amp;&amp; dataSetRow["reason"]=="HG"){
		dataSetRow["reason"]="画稿";
	}
	if(dataSetRow["reason"]!=null &amp;&amp; dataSetRow["reason"]=="YH"){
		dataSetRow["reason"]="印花";
	}
	if(dataSetRow["reason"]!=null &amp;&amp; dataSetRow["reason"]=="QT"){
		dataSetRow["reason"]="其他";
	}
	dataSetRow["remark"] = detail.get(currentRow).getRemark();	
	dataSetRow["srcformid"] = detail.get(currentRow).getSrcformid();
	dataSetRow["srcseq"] = detail.get(currentRow).getSrcseq();			
	currentRow++;
	return true;
}	
						
return false;]]></method>
